@using EntityLayer.Concrete@* 
@using EntityLayer.Entities *@
@model List<Service>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Hizmetler Listesi</h1>

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Baslik</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Title</td>
        </tr>
    }
</table>